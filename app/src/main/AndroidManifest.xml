<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="demo.yc.jovialityyc">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name="demo.yc.joviality.MyApp"
        android:allowBackup="true"
        android:icon="@drawable/main_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/main_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="demo.yc.joviality.ui.activity.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="demo.yc.joviality.ui.activity.HomeActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="demo.yc.joviality.ui.activity.CollectionActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="demo.yc.joviality.ui.activity.ImageDetailActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="demo.yc.joviality.ui.activity.UrlDetailActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name="demo.yc.joviality.ui.activity.SettingActivity">
        </activity>
        <activity android:name="demo.yc.joviality.ui.activity.SearchActivity">
        </activity>

        <service
            android:name="demo.yc.joviality.service.LoadApkFileService"
            android:exported="false">
        </service>

        <activity android:name="demo.yc.joviality.ui.activity.SkinChangeActivity">
        </activity>
    </application>

</manifest>